{
    "version": "2.0.0",
    "configurations": [
        {
            "name": "Launch",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "D:/Software/Godot_v4.3-beta1_mono_win64/Godot_v4.3-beta1_mono_win64.exe",
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopAtEntry": false,
            "args": [
				"--path",
                "${workspaceRoot}",
				"--debug-collisions",
				"--debug-paths",
				"--debug-navigation",
            ]
        },
        {
            "name": "Launch (Select Scene)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "D:/Software/Godot_v4.3-beta1_mono_win64/Godot_v4.3-beta1_mono_win64.exe",
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopAtEntry": false,
            "args": [
                "--path",
                "${workspaceRoot}",
                "${command:godot.csharp.getLaunchScene}",
				"--debug-collisions",
            ]
        },
        {
            "name": "Attach",
            "type": "coreclr",
            "request": "attach",
			"processId":"${command:pickProcess}"
        },
        {
            "name": "Launch Editor",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "D:/Software/Godot_v4.3-beta1_mono_win64/Godot_v4.3-beta1_mono_win64.exe",
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopAtEntry": false,
            "args": [
                "--path",
                "${workspaceRoot}",
                "--editor"
            ]
        }
    ]
}