; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Landless Skies"
run/main_scene="res://Scenes/Main.tscn"
config/use_custom_user_dir=true
config/features=PackedStringArray("4.2", "C#", "Forward Plus")
boot_splash/show_image=false
config/icon="res://icon.svg"

[autoload]

GameResources="*res://Scenes/ResourceManager.tscn"
CyclopsAutoload="*res://addons/cyclops_level_builder/cyclops_global_scene.tscn"

[debug]

file_logging/enable_file_logging=true

[display]

window/size/mode=4
window/size/borderless=true

[dotnet]

project/assembly_name="Landless Skies"

[editor]

naming/node_name_num_separator=2
naming/default_signal_callback_name="_{node_name}{signal_name}"
naming/default_signal_callback_to_self_name="_{signal_name}"
naming/scene_name_casing=1

[editor_plugins]

enabled=PackedStringArray("res://addons/cyclops_level_builder/plugin.cfg")

[file_customization]

folder_colors={
"res://Resources/Animations/": "green",
"res://Resources/Costumes/": "purple",
"res://Resources/Data/Characters/": "purple",
"res://Resources/Data/Weapons/": "red"
}

